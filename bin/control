#!/bin/bash -e

source $OPENSHIFT_CARTRIDGE_SDK_BASH


function start {
  echo "Starting AWS-RDS cartridge"
}

function stop {
  echo "Stopping AWS-RDS cartridge"
}

function restart {
  echo "Restarting AWS-RDS cartridge"
}

function status {
}

# Clean up any log files
function tidy() {
}

case "$1" in
  start) start ;;
  stop) stop ;;
  restart) restart ;;
  status) status ;;
  tidy) tidy ;;
esac
