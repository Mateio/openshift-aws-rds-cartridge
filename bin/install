#!/bin/bash -e

if [[ -d /usr/lib64 ]]; then
  _libdir=/usr/lib64
else
  _libdir=/usr/lib
fi

source $OPENSHIFT_CARTRIDGE_SDK_BASH

case "$1" in
    -v|--version)
      version="$2"
esac

scl enable ruby193 "bash -c 'gem install --no-ri --no-rdoc aws-sdk'"
#scl enable ruby193 "ruby $OPENSHIFT_AWSRDS_DIR/usr/bin/create_db.rb"


client_result "URL: TODO"

cart_props "connection_url=TODO"


